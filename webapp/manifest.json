{
	"_version": "1.12.0",
	"sap.app": {
		"id": "Mobilitaetskonto.Mobilitaetskonto",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "Mobilitaetskonto.Mobilitaetskonto.view.Startpage",
			"type": "XML",
			"async": true,
			"id": "Startpage"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Mobilitaetskonto.Mobilitaetskonto.i18n.i18n"
				}
			},
			"userModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"dbUserModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"salesModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"roleModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/roles.json"
			},
			"requestTableModel": {
				"type": "sap.ui.model.json.JSONModel"
			}
			
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "Mobilitaetskonto.Mobilitaetskonto.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"transition": "fade"
			},
			"routes": [{
				"name": "Startpage",
				"pattern": "Startpage",
				"target": ["Startpage"]
			}, {
				"name": "Request",
				"pattern": "Request",
				"target": ["Request"]
			}, {
				"name": "TableSales",
				"pattern": "TableSales/{Target}",
				"target": ["TableSales"]
			}, {
				"name": "DetailEmployee",
				"pattern": "DetailEmDetailEmployee/{Detail}",
				"target": ["DetailEmployee"]
			}, {
				"name": "TableRequests",
				"pattern": "TableRequests",
				"target": ["TableRequests"]
			}, {
				"name": "DetailAdministation",
				"pattern": "DetailAdministation/{Detail}",
				"target" : ["DetailAdministation"]
			},
				{
				"name": "TableEmployees",
				"pattern": "TableEmployees",
				"target": ["TableEmployees"]
				}
			],
			"targets": {
				"Startpage": {
					"viewType": "XML",
					"viewName": "Startpage"
				},
				"Request": {
					"viewType": "XML",
					"viewName": "Request"
				},
				"TableSales": {
					"viewType": "XML",
					"viewName": "TableSales"
				},
				"DetailEmployee": {
					"viewType": "XML",
					"viewName": "DetailEmployee"
				},
				"TableRequests": {
					"viewType": "XML",
					"viewName": "TableRequests"
				},
				"DetailAdministation": {
					"viewType" :"XML",
					"viewName" : "DetailAdministation"
                },
				"TableEmployees": {
					"viewType": "XML",
					"viewName": "TableEmployees"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}