<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="Mobilitaetskonto.Mobilitaetskonto.controller.SettingsAdministrationStatus" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Verwalterstatus anpassen">
				<content>
					<List id="list0" items="{ path : 'employeeTableModel>/'}">
						<items>
							<StandardListItem title="{path: 'employeeTableModel>VORNAME'} {path: 'employeeTableModel&gt;NAME'}" id="item0" detailPress="onEditPressed"
								type="Detail"
								description="MID: {path: 'employeeTableModel>MID'} Aktiv: {path: 'employeeTableModel>AKTIV'} Verwalter: {path: 'employeeTableModel>VERWALTER'}">
								<customData>
									<core:CustomData key="MID" value="{employeeTableModel>MID}"/>
								</customData>
							</StandardListItem>
						</items>
					</List>
				</content>
				<footer>
					<Toolbar width="100%" id="toolbar1">
						<content>
							<Label
								text="Hinweis: Erstmal nur testweise so unübersichtlich. Freigabewert 0 entspricht einem normalen Mitarbeiter. Werte größer 0 sind Verwalter. Eingegebene Werte dürfen nur ganze Zahlen sein."
								width="100%" id="label0"/>
						</content>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>