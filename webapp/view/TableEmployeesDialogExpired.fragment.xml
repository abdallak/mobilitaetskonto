<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
	<Dialog id="DialogExpired" title="Jahresabschluss">
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="simpleformid" layout="ResponsiveGridLayout">
				<f:content>
					<Label text="welches Jahr soll abgeschlossen werden?" id="jahrText"/>
					<DatePicker xmlns="sap.m" id="datepicker0" change="setDate" displayFormat="yyyy"/>
					<Label text="Wie viele Jahre altes Guthaben soll ablaufen?" id="alterText"/>
					<StepInput id="stepper" value="3" min="1" max="10" width="20%" />
					<!--ncoh einf체gen welches jahr das dann genau ist?    "alles 채lter als ... l채fut  jetzt ab"-->
				</f:content>
			</f:SimpleForm>
		</VBox>
		<beginButton>
			<Button text="Abschluss durchf체hren" press=".onExecuteDialog('A')"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancelButton}" press=".onAbortCloseDialog($event, 'DialogExpired')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>